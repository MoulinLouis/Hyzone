$C = "../Common.ui";

Button {
  LayoutMode: Left;
  Anchor: (Left: 2, Right: 2, Bottom: 6, Height: 40, Width: 460);
  Background: $C.@InputBoxBackground;
  Padding: (Left: 10, Right: 10, Top: 6);
  Style: ButtonStyle(
    Sounds: $C.@ButtonSounds,
  );

  Label #PlayerName {
    Style: (
      FontSize: 14,
      RenderBold: true,
      TextColor: #93844c
    );
    Text: "player-uuid";
  }
}
